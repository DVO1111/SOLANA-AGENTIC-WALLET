# Environment configuration
# Copy this to .env and fill in your values

# Solana RPC Endpoints
SOLANA_RPC_MAINNET=https://api.mainnet-beta.solana.com
SOLANA_RPC_DEVNET=https://api.devnet.solana.com
SOLANA_RPC_TESTNET=https://api.testnet.solana.com

# Active wallet address (for scripts)
WALLET_ADDRESS=

# Agent configuration
AGENT_STRATEGY=trading
AGENT_MAX_TRANSACTION_SIZE=1
AGENT_AUTO_APPROVE=true

# Logging
LOG_LEVEL=info
DEBUG=false
